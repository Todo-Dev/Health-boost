<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".activity_main_coach">

    <TextView
        android:id="@+id/textView"
        android:layout_width="201dp"
        android:layout_height="113dp"
        android:text="COACH"
        android:textSize="40sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/gotostudentactivity"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="go to student activity"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/gotocoachactivity"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="32dp"
        android:text="go to coach activity"
        app:layout_constraintTop_toBottomOf="@+id/gotostudentactivity"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="0dp" />
</androidx.constraintlayout.widget.ConstraintLayout>